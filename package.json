{
  "name": "loop",
  "productName": "Loop",
  "version": "1.6.6",
  "description": "Loop",
  "author": "Loop",
  "private": true,
  "type": "commonjs",
  "main": "out/main/index.js",
  "homepage": "./",
  "scripts": {
    "dev": "cross-env NODE_ENV=development electron-vite dev",
    "prebuild": "pnpm db:generate && node scripts/prepare-fonts.js",
    "build": "cross-env NODE_ENV=production electron-vite build && node scripts/copy-public-assets.js",
    "build:prod": "cross-env NODE_ENV=production electron-vite build && node scripts/copy-public-assets.js",
    "preview": "electron-vite preview",
    "dev:db": "prisma studio --schema=./prisma/schema.prisma",
    "build:clean": "rimraf out",
    "build:optimize": "pnpm build:strip-debug && pnpm build:cleanup",
    "build:strip-debug": "cross-env NODE_ENV=production node scripts/optimize-bundle.js || true",
    "build:cleanup": "rimraf dist/**/*.map dist/**/*.d.ts dist/**/*.js.map || true",
    "build:win": "pnpm build:prod && electron-builder --win --arch x64,arm64 --config.productName=Loop",
    "build:mac": "pnpm build:prod && electron-builder --mac --arch x64,arm64 --config.productName=Loop",
    "start": "pnpm build &&cross-env NODE_ENV=production electron .",
    "check:build-secrets": "node scripts/check-build-secrets.js",
    "start:prod": "cross-env NODE_ENV=production electron .",
    "test": "jest",
    "test:unit": "jest --testPathPattern=unit",
    "test:e2e": "jest --testPathPattern=e2e",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --fix --max-warnings=10",
    "lint:check": "eslint . --max-warnings=10",
    "lint:quiet": "eslint . --quiet",
    "lint:strict": "eslint . --max-warnings=0",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "type-check": "tsc --noEmit && tsc -p tsconfig.main.json --noEmit",
    "db:generate": "prisma generate --schema=prisma/schema.prisma",
    "db:push": "prisma db push --schema=prisma/schema.prisma",
    "db:migrate": "prisma migrate dev --schema=prisma/schema.prisma",
    "db:reset": "prisma migrate reset --schema=prisma/schema.prisma",
    "db:studio": "prisma studio --schema=prisma/schema.prisma",
    "pack": "pnpm build && electron-builder --dir",
    "dist": "pnpm prebuild && pnpm build && electron-builder",
    "dist:all": "pnpm build",
    "dist:mac": "pnpm prebuild && pnpm build && electron-builder --mac --x64 --arm64 --publish never",
    "dist:mac-local": "pnpm prebuild && pnpm build && electron-builder --mac --x64 --arm64 --publish always",
    "dist:win": "pnpm prebuild && pnpm build && electron-builder --win --x64 --arm64 --publish never",
    "dist:win:web": "pnpm prebuild && pnpm build && electron-builder --win --x64 --arm64 -c.win.target=nsis-web --publish never",
    "dist:linux": "pnpm prebuild && pnpm build && electron-builder --linux",
    "publish": "pnpm build && electron-builder --publish=always",
    "clean": "rimraf out node_modules/.cache",
    "clean:all": "rimraf out node_modules",
    "postinstall": "electron-builder install-app-deps && pnpm db:generate",
    "debug:main": "cross-env NODE_ENV=development electron --inspect=5858 .",
    "debug:test": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "clear": "clear;clear",
    "push": "git push origin main"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@harshtalks/slash-tiptap": "^1.3.0",
    "@prisma/client": "^6.17.0",
    "@react-oauth/google": "^0.12.2",
    "@tiptap/core": "^2.25.0",
    "@tiptap/extension-bubble-menu": "^2.24.0",
    "@tiptap/extension-character-count": "^2.24.2",
    "@tiptap/extension-color": "^2.24.2",
    "@tiptap/extension-focus": "^2.24.2",
    "@tiptap/extension-highlight": "^2.26.1",
    "@tiptap/extension-image": "^2.24.0",
    "@tiptap/extension-link": "^2.24.0",
    "@tiptap/extension-placeholder": "^2.24.0",
    "@tiptap/extension-task-item": "^2.25.0",
    "@tiptap/extension-task-list": "^2.25.0",
    "@tiptap/extension-text-align": "^2.24.2",
    "@tiptap/extension-text-style": "^2.24.2",
    "@tiptap/extension-typography": "^2.24.0",
    "@tiptap/extension-underline": "^2.24.0",
    "@tiptap/pm": "^2.26.1",
    "@tiptap/react": "^2.24.0",
    "@tiptap/starter-kit": "^2.24.0",
    "@tiptap/suggestion": "^2.25.0",
    "async-lock": "^1.4.1",
    "axios": "^1.12.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "16.4.5",
    "driver.js": "^1.3.6",
    "electron-store": "^8.1.0",
    "electron-updater": "^6.6.2",
    "google-auth-library": "^10.3.0",
    "googleapis": "^156.0.0",
    "highlight.js": "^11.11.1",
    "keytar": "^7.9.0",
    "lucide-react": "^0.539.0",
    "node-mac-permissions": "^2.5.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^10.1.0",
    "recharts": "^3.2.1",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1",
    "tailwind-merge": "^3.3.1",
    "tippy.js": "^6.3.7",
    "vitest": "^3.2.4",
    "zod": "^4.1.9",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^2.1.4",
    "@fontsource/nanum-gothic": "^5.2.7",
    "@fontsource/noto-sans-jp": "^5.2.8",
    "@fontsource/noto-sans-kr": "^5.2.8",
    "@noonnu/gangwon-edu-otf-bold-a": "^0.1.0",
    "@noonnu/gangwon-edu-saeeum-otf-medium-a": "^0.1.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/async-lock": "^1.4.2",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "@vitejs/plugin-react": "^5.0.3",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "electron": "38.1.2",
    "electron-builder": "^26.0.12",
    "electron-vite": "^4.0.0",
    "eslint": "^8.57.1",
    "glob": "^11.0.3",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "postcss": "^8.5.6",
    "pretendard": "^1.3.9",
    "pretendard-jp": "^1.3.9",
    "prisma": "^6.17.1",
    "react-router-dom": "^7.9.1",
    "rimraf": "^6.0.1",
    "tailwind-scrollbar-hide": "^4.0.0",
    "tailwindcss": "^3.4.17",
    "ts-prune": "^0.10.3",
    "tsx": "^4.20.3",
    "typescript": "^5"
  },
  "packageManager": "pnpm@10.18.3",
  "pnpm": {
    "overrides": {
      "@types/react": "^19.0.0",
      "@types/react-dom": "^19.0.0"
    },
    "ignoredBuiltDependencies": [
      "unrs-resolver"
    ],
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "@prisma/client",
      "@prisma/engines",
      "@swc/core",
      "@tailwindcss/oxide",
      "better-sqlite3",
      "electron",
      "electron-chromedriver",
      "electron-winstaller",
      "esbuild",
      "keytar",
      "node-mac-permissions",
      "prisma",
      "protobufjs",
      "sharp",
      "spectron"
    ]
  }
}
